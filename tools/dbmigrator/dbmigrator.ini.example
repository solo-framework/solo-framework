;***************************************
;
; Конфигурационный файл MySQL мигратора
;
;***************************************

; Адрес сервера
host = "localhost"

; Имя пользователя БД
user = "root"

; Пароль
pass = "toor"

; Имя БД
dbname = "testmigration"

; Путь к директории с миграциями
migrationStorage = "tests/data/storage"

; Путь к директории с временной миграцией
migrationTempPath = "tests/data/storage/dev"

; Путь к директории с бинарыми логами mysql
; Определяется в конфиге mysql
; [mysqld]
; log_bin           = {path}
; expire_logs_days  = {N}
; RESET MASTER - удалить все логи
binaryLogPath = "/var/log/mysql"

